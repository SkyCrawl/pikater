      <value>
        <object type="boolean">false</object>
      </value>
    </OptionDefault>
    <OptionDefault>
      <name>B</name>
      <description>A NominalToBinary filter will NOT automatically be used</description>
      <value>
        <object type="boolean">false</object>
      </value>

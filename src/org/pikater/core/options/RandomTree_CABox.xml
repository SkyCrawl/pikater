<Box>
  <ontology>org.pikater.core.ontology.description.ComputingAgent</ontology>
  <options>
    <OptionDefault>
      <name>K</name>
      <description>Sets the number of randomly chosen attributes</description>
      <value>
        <object type="int">1</object>
      </value>
      <interval>
        <minValue type="int">1</minValue>
        <maxValue type="int">50</maxValue>
      </interval>
      <list>
        <list/>
      </list>
    </OptionDefault>
    <OptionDefault>
      <name>M</name>
      <description>The minimum total weight of the instances in a leaf</description>
      <value>
        <object type="int">0</object>
      </value>
      <interval>
        <minValue type="int">1</minValue>
        <maxValue type="int">100</maxValue>
      </interval>
      <list>
        <list/>
      </list>
    </OptionDefault>
    <OptionDefault>
      <name>Q</name>
      <description>The random number seed used for selecting attributes</description>
      <value>
        <object type="int">1</object>
      </value>
      <interval>
        <minValue type="int">1</minValue>
        <maxValue type="int">2147483647</maxValue>
      </interval>
      <list>
        <list/>
      </list>
    </OptionDefault>
  </options>
  <inputSlots>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <description>Training data</description>
      <ontologyFieldName>trainingData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <description>Validation data</description>
      <ontologyFieldName>testingData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <ontologyFieldName>validationData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
  </inputSlots>
  <outputSlots>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>AGENT_SLOT</type>
      <sex>PRODUCENT</sex>
      <description>data computed</description>
    </org.pikater.core.dataStructures.slots.Slot>
  </outputSlots>
  <name>RandomTree</name>
  <agentClass>org.pikater.core.agents.experiment.computing.Agent_WekaCA</agentClass>
  <picture>picture3.jpg</picture>
  <description>Random Tree Method</description>
</Box>

<Box>
  <ontology>org.pikater.core.ontology.description.ComputingAgent</ontology>
  <options>
    <OptionDefault>
      <name>F</name>
      <description></description>
      <value>
        <object type="float">5.0</object>
      </value>
      <interval>
        <minValue type="float">1.0</minValue>
        <maxValue type="float">100.0</maxValue>
      </interval>
    </OptionDefault>
  </options>
  <inputSlots>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <description>Training data</description>
      <ontologyFieldName>trainingData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <description>Validation data</description>
      <ontologyFieldName>testingData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <ontologyFieldName>validationData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
  </inputSlots>
  <outputSlots>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>AGENT_SLOT</type>
      <sex>PRODUCENT</sex>
      <description>data computed</description>
    </org.pikater.core.dataStructures.slots.Slot>
  </outputSlots>
  <name>CrossValidation-Method</name>
  <agentClass>org.pikater.core.agents.experiment.computing.Agent_WekaCA</agentClass>
  <picture>picture3.jpg</picture>
  <description>Computing agent used for training neural networks deterministic library WEKA. As a training method is used default Cross Validation WEKA method.</description>

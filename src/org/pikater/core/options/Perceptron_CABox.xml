<Box>
  <ontology>org.pikater.core.ontology.description.ComputingAgent</ontology>
  <options>
    <OptionDefault>
      <name>L</name>
      <description>Learning rate</description>
      <value>
        <object type="float">0.3</object>
      </value>
      <interval>
        <minValue type="float">0.0</minValue>
        <maxValue type="float">1.0</maxValue>
      </interval>
      <list>
        <list/>
      </list>
    </OptionDefault>
    <OptionDefault>
      <name>N</name>
      <description>Number of epochs to train through</description>
      <value>
        <object type="int">1</object>
      </value>
      <interval>
        <minValue type="int">1</minValue>
        <maxValue type="int">1000</maxValue>
      </interval>
      <list>
        <list/>
      </list>
    </OptionDefault>
    <OptionDefault>
      <name>S</name>
      <description>Seed of the random number generator</description>
      <value>
        <object type="int">0</object>
      </value>
      <interval>
        <minValue type="int">0</minValue>
        <maxValue type="int">2147483647</maxValue>
      </interval>
      <list>
        <list/>
      </list>
    </OptionDefault>
    <OptionDefault>
      <name>C</name>
      <description>Normalizing a numeric class will NOT be done</description>
      <value>
        <object type="boolean">false</object>
      </value>
    </OptionDefault>
  </options>
  <inputSlots>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <description>Training data</description>
      <ontologyFieldName>trainingData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <description>Validation data</description>
      <ontologyFieldName>testingData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <ontologyFieldName>validationData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
  </inputSlots>
  <outputSlots>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>AGENT_SLOT</type>
      <sex>PRODUCENT</sex>
      <description>data computed</description>
    </org.pikater.core.dataStructures.slots.Slot>
  </outputSlots>
  <name>Perceptron</name>
  <agentClass>org.pikater.core.agents.experiment.computing.Agent_WekaCA</agentClass>
  <picture>picture3.jpg</picture>
  <description>Perceptron Method</description>
</Box>

<Box>
  <ontology>org.pikater.core.ontology.description.ComputingAgent</ontology>
  <options>
    <OptionDefault>
      <name>I</name>
      <description>Set the number of folder to use in the computing of the mutual information</description>
      <value>
        <object type="int">5</object>
      </value>
      <interval>
        <minValue type="int">1</minValue>
        <maxValue type="int">100</maxValue>
      </interval>
      <list>
        <list/>
      </list>
    </OptionDefault>
    <OptionDefault>
      <name>G</name>
      <description>Set the number of attempts of generalisation</description>
      <value>
        <object type="int">5</object>
      </value>
      <interval>
        <minValue type="int">1</minValue>
        <maxValue type="int">50</maxValue>
      </interval>
      <list>
        <list/>
      </list>
    </OptionDefault>
  </options>
  <inputSlots>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <description>Training data</description>
      <ontologyFieldName>trainingData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <description>Validation data</description>
      <ontologyFieldName>testingData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>DATA_SLOT</type>
      <sex>CONSUMENT</sex>
      <ontologyFieldName>validationData</ontologyFieldName>
    </org.pikater.core.dataStructures.slots.Slot>
  </inputSlots>
  <outputSlots>
    <org.pikater.core.dataStructures.slots.Slot>
      <type>AGENT_SLOT</type>
      <sex>PRODUCENT</sex>
      <description>data computed</description>
    </org.pikater.core.dataStructures.slots.Slot>
  </outputSlots>
  <name>NNge</name>
  <agentClass>org.pikater.core.agents.experiment.computing.Agent_WekaCA</agentClass>
  <picture>picture3.jpg</picture>
  <description>NNge Method</description>
</Box>

<pikater.utility.boxTypes.Box>
  <name>CrossValidation-Method</name>
  <agentName>pikater/Agent_CrossValidation</agentName>
  <type>Computing</type>
  <picture>picture3.jpg</picture>
  <description>Computing agent which used for training neural networks deterministic library WEKA. As a training method is used default Cross Validation WEKA method.</description>
  <parameters>
    <pikater.utility.pikaterDatabase.tables.experiments.parameters.UIntegerParameter>
      <name>F</name>
      <value>5</value>
      <range>
        <max>100</max>
        <min>1</min>
      </range>
    </pikater.utility.pikaterDatabase.tables.experiments.parameters.UIntegerParameter>
  </parameters>
  <inputSlots>
    <pikater.utility.pikaterDatabase.tables.experiments.slots.DataConsumerSlot>
      <slotName>Training data</slotName>
    </pikater.utility.pikaterDatabase.tables.experiments.slots.DataConsumerSlot>
    <pikater.utility.pikaterDatabase.tables.experiments.slots.DataConsumerSlot>
      <slotName>Testing data</slotName>
    </pikater.utility.pikaterDatabase.tables.experiments.slots.DataConsumerSlot>
    <pikater.utility.pikaterDatabase.tables.experiments.slots.DataConsumerSlot>
      <slotName>Validation data</slotName>
    </pikater.utility.pikaterDatabase.tables.experiments.slots.DataConsumerSlot>
  </inputSlots>
  <outputSlots>
    <pikater.utility.pikaterDatabase.tables.experiments.slots.DataProviderSlot>
      <slotName>Computed data</slotName>
    </pikater.utility.pikaterDatabase.tables.experiments.slots.DataProviderSlot>
  </outputSlots>
</pikater.utility.boxTypes.Box>
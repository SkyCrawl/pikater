<pikater.utility.boxTypes.Box>
  <name>Random-Searcher</name>
  <agentName>pikater/Agent_RandomSearch</agentName>
  <type>Searcher</type>
  <picture>picture1.jpg</picture>
  <description>Searcher is using to find values of parameters for computing agents. For searchthe solution is used random generator.</description>
  <parameters>
    <pikater.utility.pikaterDatabase.tables.experiments.parameters.UDoubleParameter>
      <name>E</name>
      <value>0.01</value>
      <range>
        <min>0.0</min>
        <max>1.0</max>
      </range>
    </pikater.utility.pikaterDatabase.tables.experiments.parameters.UDoubleParameter>
    <pikater.utility.pikaterDatabase.tables.experiments.parameters.UIntegerParameter>
      <name>M</name>
      <value>10</value>
      <range>
        <max>100000</max>
        <min>1</min>
      </range>
    </pikater.utility.pikaterDatabase.tables.experiments.parameters.UIntegerParameter>
  </parameters>
  <inputSlots/>
  <outputSlots>
    <pikater.utility.pikaterDatabase.tables.experiments.slots.ParametersProviderSlot>
      <slotName>Found parameters</slotName>
    </pikater.utility.pikaterDatabase.tables.experiments.slots.ParametersProviderSlot>
  </outputSlots>
</pikater.utility.boxTypes.Box>
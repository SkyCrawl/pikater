<experiments>
    <experiment timeout="10000" get_results="after_each_computation" save_results="yes">        
		<method name="EASearch">
			<parameter name="E" value="0.01" />
        	<parameter name="M" value="1000" />
			<parameter name="P" value="10" />
			<parameter name="X" value="0.0" />
			<parameter name="L" value="0.1" />
			<parameter name="I" value="300" />
			<parameter name="T" value="1.0" />
			<parameter name="F" value="0.2" />
        </method>
        
        <evaluation name="CrossValidation">
          <parameter name="F" value="10" />        
        </evaluation>
        
		<dataset train="dermatology.arff" test="dermatology.arff" />
		
        <agent type="SMO">
        	<parameter name="C" value="0.05" />
        	<parameter name="G" value="0.3" />
			<parameter name="R" value="True" />
    	</agent>
        	
   </experiment>
       
</experiments>

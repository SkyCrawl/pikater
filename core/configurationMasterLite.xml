<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <agents>
        <agent type="org.pikater.core.agents.system.Agent_Planner" name="Planner">
        </agent>

        <agent type="org.pikater.core.agents.system.Agent_Manager" name="Manager">
            <arguments>
                <argument key="no_xml_output" value="true"/>
            </arguments>
        </agent>

        <agent type="org.pikater.core.agents.system.Agent_DataManager" name="DataManager">
        </agent>

        <agent type="org.pikater.core.agents.system.Agent_MetadataQueen" name="MetadataQueen">
        </agent>

        <agent type="org.pikater.core.agents.system.Agent_ManagerAgent" name="ManagerAgent">
        </agent>

        <agent type="jade.tools.sniffer.Sniffer" name="sniffrik">
            <arguments>
                <argument key="sniffed" value="Manager" sendOnlyValue="true" />
            </arguments>
        </agent>

        <agent type="org.pikater.core.agents.system.Agent_GuiAgentsCommunicator" name="GuiAgentsCommunicator">
        </agent>

        <!--
        <agent type="org.pikater.core.agents.system.Agent_GUIKlara" name="GUIKlara">
        </agent>
        -->

        <agent type="org.pikater.core.agents.system.Agent_AgentInfoManager" name="AgentInfoManager">
        </agent>

    </agents>
</configuration>
